generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// schema.prisma

model Voter {
  id          Int      @id @default(autoincrement())
  username    String
  email       String   @unique
  password    String
  full_name   String
  DOB         String
  national_id String   @unique
  gender      String
  city        String
  voted       Boolean?
}
